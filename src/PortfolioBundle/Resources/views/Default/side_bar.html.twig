
{% for article in articles %}
    <img src="{{ article.image }}" alt="" class="img-responsive">

    <div class="side_bar_titre">
        <a href="{{ path('portfolio_show_article', {'id': article.id}) }}">
            <h3>{{ article.title }}</h3>
        </a>
        <p>{{ article.publishedAt|date('d/m/y') }}</p>
    </div>
{% endfor %}



















